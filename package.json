{
  "name": "base-mercado-liebre",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "client:start": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "server": "nodemon ./src/bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client:start\" ",
    "create:db": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "reset:db": "sequelize db:drop && npm run create:db",
    "dev:reset": "npm run reset:db && nodemon ./src/bin/www"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.22.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.1",
    "sequelize-cli": "^6.5.1"
  }
}
